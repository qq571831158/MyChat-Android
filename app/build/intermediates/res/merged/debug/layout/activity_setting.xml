<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_setting"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.apple.mychatqq.activity.profile.SettingActivity"
    android:background="@color/backgroundColor">
    <ListView
        android:id="@+id/layout_setting_list1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#FFF"
        android:entries="@array/list1">

    </ListView>
    <ListView
        android:id="@+id/layout_setting_list2"
        android:layout_width="match_parent"
        android:layout_height="144dp"
        android:layout_below="@id/layout_setting_list1"
        android:layout_marginTop="20dp"
        android:background="#FFF"
        android:entries="@array/list2">
    </ListView>
</RelativeLayout>
